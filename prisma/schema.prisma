// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  password String
}

model Aboutme {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  content String
}

model Contact {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  url  String
}
